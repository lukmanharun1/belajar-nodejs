    "paths": {
        "/mahasiswa": {
            "get": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Get All Mahasiswa",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Sukses"
                    }
                }

            },
            "post": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Post Mahasiswa",
                "consumes": "multipart/form-data",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "nim": {
                                        "type": "string"
                                    },
                                    "nama": {
                                        "type": "string"
                                    },
                                    "kd_jurusan": {
                                        "type": "string"
                                    },
                                    "alamat": {
                                        "type": "string"
                                    },
                                    "angkatan": {
                                        "type": "string"
                                    },
                                    "foto": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Sukses"
                    }
                }

            }
