        "/mahasiswa/search?keyword=": {
            "get": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Search Mahasiswa",
                "description": "",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "keyword",
                        "schema": {
                            "type": "string"
                        },
                        "description": "NIM/ Nama Mahasiswa"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sukses"
                    }
                }
            }
