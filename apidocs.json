                }
            },
            "put": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Update Mahasiswa",
                "parameters": [
                    {
                        "in": "path",
                        "name": "nim",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "NIM Mahasiswa"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "string",
                                "properties": {
                                    "nama": {
                                        "type": "string"
                                    },
                                    "kd_jurusan": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }

            },
            "delete": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Delete Mahasiswa",
                "parameters": [
                    {
                        "in": "path",
                        "name": "nim",
                        "schema": {
                            "type": "integer"
