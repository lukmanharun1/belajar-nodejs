        "/mahasiswa/{nim}": {
            "get": {
                "tags": [
                    "Mahasiswa"
                ],
                "summary": "Get One Mahasiswa",
                "description": "",
                "parameters": [
                    {
                        "in": "path",
                        "name": "nim",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "Nim Mahasiswa"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sukses"
